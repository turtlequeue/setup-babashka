{
  "name": "setup-babashka",
  "version": "1.7.0",
  "private": false,
  "description": "setup babashka action",
  "main": "src/setup-babashka.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --license LICENCE",
    "all": "npm run build && npm run format && npm run lint && npm run package",
    "test": "jest"
  },
  "test": {
    "type": "git",
    "url": "git+https://github.com/turtlequeue/setup-babashka.git"
  },
  "keywords": [
    "actions",
    "clojure",
    "babashka",
    "github",
    "setup"
  ],
  "author": "nha",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.8",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-formatter": "^7.2.2"
  }
}
